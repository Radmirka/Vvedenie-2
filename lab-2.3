#include <iostream>
#include <cmath>
using namespace std;
int main() 
{
    cout<<"Введите точки"<<endl;
    int a,ac,bc,b,c,pr;
    cin>>a>>b>>c;
    if (((c>a)&&(c>b))||((c<a)&&(c<b))){
        cout<<"Ошибка! Не верно условие ввода! "<<endl;}
    else {
        ac=abs(a-c);
        bc=abs(b-c);
        pr=ac*bc;
    cout<<"Произведение равно "<<pr<<endl;
    }
    return 0;
}
